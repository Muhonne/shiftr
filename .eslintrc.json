{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "quotes": 0,
    "space-before-function-paren": 0,
    "react/jsx-filename-extension": 0,
    "comma-dangle": 0
  }
}
